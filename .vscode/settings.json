{
  "files.associations": {
    "*.json": "jsonc"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    // "**/node_modules": true,
    "**/.next": true,
    "**/*.log": true,
    "**/dist": false,
    "**/.rush": false,
    "**/temp": false,
    "**/tsconfig.tsbuildinfo": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.wordWrap": "on",
  "cSpell.words": [
    "Aptos",
    "Bytecode",
    "bytecodes",
    "chakra",
    "codebox",
    "commitlint",
    "devnet",
    "esbuild",
    "execa",
    "gitops",
    "ipfs",
    "lparen",
    "monokai",
    "nestjs",
    "nftstorage",
    "noconflict",
    "nodereal",
    "pnpm",
    "rparen",
    "svgr",
    "tanstack",
    "totejs",
    "tracemove",
    "verfiy",
    "vout"
  ]
}
